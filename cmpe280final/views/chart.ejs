					        }).error(function(error){
					            alert("error");
					        });
					};
					
			$scope.charts = function() {
				$http({
				  	url: '/chart', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/chart';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};
					
			$scope.dashboard = function() {
				$http({
