  		function personController($scope,$http,$location) {
		    
			$scope.signOut = function() {
				$http({
				  	url: '/signout', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/signin';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};
					
			$scope.highMap = function() {
				$http({
				  	url: '/highmap', 
