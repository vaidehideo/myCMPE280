<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Pizzalelopizza a pizza admin page to render analytics">
    <meta name="author" content="CMPE280 Team10">
    <meta name="keyword" content="Creative, Dashboard, Admin, Pizza, Analytics, Food, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>Analytics</title>
    
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
	<script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
	<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>

    <!-- Bootstrap CSS -->    
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <!-- bootstrap theme -->
    <link href="css/bootstrap-theme.css" rel="stylesheet">
    <!--external css-->
    <!-- font icon -->
    <link href="css/elegant-icons-style.css" rel="stylesheet" />
    <link href="css/font-awesome.min.css" rel="stylesheet" />    
    <!-- Custom styles -->
	<link rel="stylesheet" href="css/fullcalendar.css">
	<link href="css/widgets.css" rel="stylesheet">
    <link href="css/style.css" rel="stylesheet">
    <link href="css/style-responsive.css" rel="stylesheet" />
	<link href="css/jquery-ui-1.10.4.min.css" rel="stylesheet">
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
      <script src="js/lte-ie7.js"></script>
    <![endif]-->
  </head>

  <body>
  <div  ng-app="" ng-controller="personController">
  <!-- container section start -->
  <section id="container" class="">
     
      
      <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"></div>
            </div>

            <!--logo start-->
            <a href="" ng-click="dashboard()" class="logo">Pizza <span class="lite">Admin</span></a>
            <!--logo end-->


            <div class="top-nav notification-row">                
                <!-- notificatoin dropdown start-->
                <ul class="nav pull-right top-menu">
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <span class="profile-ava">
                                <img alt="" src="img/avatar1_small.jpg">
                            </span>
                            <span class="username">Pizza Admin</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>
                            <li>
                                <a href="" ng-click="signOut()"><i class="icon_key_alt"></i> Log Out</a>
                                <!-- input type="button" ng-click="signIn()" name="signin" value="Sign In" id="signin" tabindex="3" class="btn btn-primary btn-lg btn-block"-->
                            </li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!-- notificatoin dropdown end-->
            </div>
      </header>      
      <!--header end-->

      <!--sidebar start-->
      <aside>
          <div id="sidebar"  class="nav-collapse ">
              <!-- sidebar menu start-->
              <ul class="sidebar-menu">                
                  <li class="active">
                      <a class="" href="" ng-click="dashboard()">
                          <i class="icon_house_alt"></i>
                          <span>Dashboard</span>
                      </a>
                  </li>
                  <li>                     
                      <a class="" href="" ng-click="toppingscharts()">
                          <i class="icon_piechart"></i>
                          <span>Popular Toppings</span>                          
                      </a>                                         
                  </li>
                  <li>                     
                      <a class="" href="" ng-click="vnvcharts()">
                          <i class="icon_piechart"></i>
                          <span>Veg vs Non-Veg</span>                          
                      </a>                                         
                  </li>
                  <li>                     
                      <a class="" href="" ng-click="crustcharts()">
                          <i class="icon_piechart"></i>
                          <span>Crust Popularity</span>                          
                      </a>                                         
                  </li>
                  <li>                     
                      <a class="" href="" ng-click="highMap()">
                          <i class="icon_globe"></i>
                          <span>Sales Map</span>                          
                      </a>                                         
                  </li>
              </ul>
              <!-- sidebar menu end-->
          </div>
      </aside>
      <!--sidebar end-->
      
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper">            
              <!--overview start-->
			  <div class="row">
				<div class="col-lg-12">
					<h3 class="page-header"><i class="fa fa-laptop"></i> Dashboard</h3>
					<ol class="breadcrumb">
						<li><i class="fa fa-home"></i><a href="" ng-click="dashboard()">Home</a></li>
						<li><i class="fa fa-laptop"></i>Dashboard</li>						  	
					</ol>
				</div>
			</div>
              
            <div class="row">
				<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
					<div class="info-box blue-bg">
						<i class="fa fa-money"></i>
						<div class="count">${{sales}}</div>
						<div class="title">Sales</div>						
					</div><!--/.info-box-->			
				</div><!--/.col-->
				
				<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
					<div class="info-box brown-bg">
						<i class="fa fa-shopping-cart"></i>
						<div class="count">{{orders}}</div>
						<div class="title">Orders</div>						
					</div><!--/.info-box-->			
				</div><!--/.col-->	
				
				<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
					<div class="info-box dark-bg">
						<i class="fa fa-thumbs-o-up"></i>
						<div class="count" ng-model="likes">{{likes}}</div>
						<div class="title">Likes</div>						
					</div><!--/.info-box-->			
				</div><!--/.col-->
				
				<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
					<div class="info-box green-bg">
						<i class="fa fa-cubes"></i>
						<div class="count">{{count}}</div>
						<div class="title">Website Visits</div>					
					</div><!--/.info-box-->			
				</div><!--/.col-->
				
			</div><!--/.row-->
		
			
           <div class="row">
		    <div class="col-lg-9 col-md-12">
					
					
				</div>
              <div class="col-md-3">
		</section>
      </section>
      <!--main content end-->
  </section>
  <!-- container section start -->
  
  </div>
  </body>

    <!-- javascripts -->
    <script src="js/jquery.js"></script>
	<script src="js/jquery-ui-1.10.4.min.js"></script>
    <script src="js/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="js/jquery-ui-1.9.2.custom.min.js"></script>
    <!-- bootstrap -->
    <script src="js/bootstrap.min.js"></script>
    <!-- nice scroll -->
    <script src="js/jquery.scrollTo.min.js"></script>
    <script src="js/jquery.nicescroll.js" type="text/javascript"></script>
    <!--script for this page only-->

	<script src="js/jquery.rateit.min.js"></script>
    <!--custome script for all page-->
    <script src="js/scripts.js"></script>
    
  <script>
  
  
  		function personController($scope,$http,$location) {
  			loadData();
  			function loadData(){
				//alert("hi2");
		    	$http({
		    		method: 'Get',
		            url: '/getDashboardData',
		         }).success(function(response){
		         		var jsonlikes = JSON.parse(response.likes);
		         		var jsoncount = JSON.parse(response.count);
		         		var jsonsales = JSON.parse(response.sales);
		         		var jsonorders = JSON.parse(response.orders);
		         		//console.log(response.sales);
		         		//console.log(response.orders);
		           		$scope.likes= jsonlikes[0].password;
		           		$scope.count = jsoncount[0].password;
		           		$scope.sales= jsonsales[0].sale;
		           		$scope.orders = jsonorders[0].wcount;
		            }).error(function(error){
		            alert("error");
		        });
		    };
		    
			$scope.signOut = function() {
				$http({
				  	url: '/signout', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/signin';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};
					
			$scope.highMap = function() {
				$http({
				  	url: '/highmap', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/highmap';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};
					
			$scope.crustcharts = function() {
				$http({
				  	url: '/crustchart', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/crustchart';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};
					
			$scope.vnvcharts = function() {
				$http({
				  	url: '/vegnonvegchart', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/vegnonvegchart';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};					
					
			$scope.toppingscharts = function() {
				$http({
				  	url: '/populartoppings', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/populartoppings';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};
					
			$scope.dashboard = function() {
				$http({
				  	url: '/dashboard', 
				  	method: "GET",
				 	}).success(function(response){
					           
					            //alert(JSON.stringify(response));
					            window.location = '/dashboard';
					          
					        }).error(function(error){
					            alert("error");
					        });
					};		
			
		}

  </script>
  
</html>
