		         		arr.push(['Jalapeno',jsonjal[0].Jalapeno]);
		         		arr.push(['GreenPeppers',jsonGP[0].GreenPeppers]);
		         		arr.push(['Cheese',jsonches[0].Cheese]);
		         		//console.log(arr[3]);
		         		//$scope.tarray = arr;
