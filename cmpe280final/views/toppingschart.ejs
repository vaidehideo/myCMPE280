  			function loadToppingData(){
				//alert("hi2");
		    	$http({
		    		method: 'Get',
		            url: '/getToppingsData',
		         }).success(function(response){
		         		var jsononions = JSON.parse(response.Onions);
		         		var jsonmush = JSON.parse(response.Mushrooms);
		         		var jsonpop = JSON.parse(response.Pepperoni);
		         		var jsonham = JSON.parse(response.Ham);
